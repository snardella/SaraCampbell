{
  "components/common/paginator": "<div class=\"pagination\">\r\n    <ul class=\"pagination-list\">\r\n\t\t{{#gt links.length 0}}\r\n        <li class=\"pagination-item item-results\">{{{lang 'common.paginator.page_of' start=start end=end total=totalCount}}}</li>\r\n\t\t{{else}}\r\n            <li class=\"pagination-item item-results\">{{{lang 'common.paginator.page_of_alt' start=start end=totalCount total=totalCount}}}</li>\r\n        {{/gt}}\r\n        {{#if previous}}\r\n            <li class=\"pagination-item pagination-item--previous\">\r\n                <a class=\"pagination-link\" href=\"{{previous}}\" {{#unless reload}}data-faceted-search-facet{{/unless}}>\r\n                    <i class=\"icon\" aria-hidden=\"true\">\r\n                        <svg>\r\n                            <use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-chevron-left\"></use>\r\n                        </svg>\r\n                    </i>\r\n                    {{lang 'common.previous'}}\r\n                </a>\r\n            </li>\r\n        {{/if}}\r\n        {{#each links}}\r\n            {{#if this.number '==' ../current}}\r\n                <li class=\"pagination-item pagination-item--current\">\r\n                    <a class=\"pagination-link\" href=\"{{this.url}}\" {{#unless reload}}data-faceted-search-facet{{/unless}}>{{this.number}}</a>\r\n                </li>\r\n            {{else}}\r\n                <li class=\"pagination-item\">\r\n                    <a class=\"pagination-link\" href=\"{{this.url}}\" {{#unless reload}}data-faceted-search-facet{{/unless}}>{{this.number}}</a>\r\n                </li>\r\n            {{/if}}\r\n        {{/each}}\r\n\r\n        {{#if next}}\r\n            <li class=\"pagination-item pagination-item--next\">\r\n                <a class=\"pagination-link\" href=\"{{next}}\" {{#unless reload}}data-faceted-search-facet{{/unless}}>\r\n                    \r\n                    <i class=\"icon\" aria-hidden=\"true\">\r\n                        <svg>\r\n                            <use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-chevron-right\"></use>\r\n                        </svg>\r\n                    </i>\r\n                </a>\r\n            </li>\r\n        {{/if}}\r\n    </ul>\r\n</div>\r\n"
}